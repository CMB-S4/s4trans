#!/usr/bin/env python
from s4trans import s4tools
import numpy

obs_seq = s4tools.load_obs_seq()

obs_key = '40-7-12'
width = 12
scan = 'RISING'

k = numpy.where(obs_seq[scan]['obs_seq'] == obs_key)[0][0]

print(type(k))
print(obs_seq[scan]['filename'])

files = obs_seq[scan]['filename'][k-12: k+12]
print(files)
